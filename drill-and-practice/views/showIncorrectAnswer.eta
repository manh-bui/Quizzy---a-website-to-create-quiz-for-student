<% layout('./layouts/layout.eta') %>
<style>
    form {
        display: inline-block;
    }
</style>

<h1>Topic: <%= it.topic[0].name %> </h1>

<h2>Question: <%= it.question[0].question_text %></h2>

<h3>Incorrect!</h3>

<h3> The correct option was <%= it.correctOption[0].option_text %></h3>

<h4><a href="/quiz/<%= it.topic[0].id %>" > Next question </a></h4>